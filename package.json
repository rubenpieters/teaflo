{
  "name": "teaflo",
  "version": "0.0.1",
  "description": "teaflo",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenpieters/teaflo"
  },
  "license": "MIT",
  "author": "rubenpieters",
  "scripts": {
    "setup": "npm install -g npx@9.7.1 && npm install",
    "tslint": "npx tslint -c ./tslint.json --project ./tsconfig.json --format stylish",
    "webpack:dev": "npx webpack --watch --config webpack.dev.config.js",
    "webpack:prod": "npx webpack --config webpack.production.config.js",
    "dev:test": "npx ts-node -r tsconfig-paths/register test/main.ts",
    "heroku-prebuild": "npm run-script setup",
    "heroku-postbuild": "npm run-script webpack:prod"
  },
  "dependencies": {
    "express": "^4.16.4",
    "immutable-assign": "^2.0.6",
    "phaser-ce": "^2.10.3"
  },
  "devDependencies": {
    "@types/expect": "^1.20.3",
    "@types/express": "^4.16.0",
    "@types/node": "^10.1.2",
    "browser-sync": "^2.23.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.18",
    "expect": "^23.3.0",
    "expose-loader": "^0.7.4",
    "html-webpack-plugin": "^2.30.1",
    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.3.1",
    "tsconfig-paths-webpack-plugin": "^3.0.4",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.9.2",
    "typestrict": "0.0.9",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.12.0"
  }
}
